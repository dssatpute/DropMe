<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Downloads</title>
  </head>
  <body>
    <h2>Ready to Download</h2>
    <div class="info">
      <div class="name">
        <h3>Name</h3>
        <span>{{fileName}}</span>
      </div>
      <div class="size">
        <h3>Size</h3>
        <span>{{fileSize}}</span>
      </div>
      <div class="uuis">
        <h3>UUID</h3>
        <span>{{uuid}}</span>
      </div>
    </div>
    <div class="download">
      <a href={{downloadLink}}>Download</a>
    </div>
    <h3>{{downloadLink}}</h3>
    <h3>Hello   </h3>
  </body>
</html>